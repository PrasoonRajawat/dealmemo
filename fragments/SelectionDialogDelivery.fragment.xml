	<!--Added MultiSelect on 19/05/2022 by Dhiraj Sarang-->


 <Dialog title="Select Delivery Code" type="Message" contentWidth="40%" class="sapUiContentPadding">

<List mode="MultiSelect" items="{vendorContractModel>/deliveryCodeList}" >
		<CustomListItem>
			<HBox>
				<VBox class="sapUiSmallMarginBegin sapUiSmallMarginTopBottom" >
					<ObjectIdentifier title="{vendorContractModel>Mstcdnm}" text = "{vendorContractModel>Mstcd}"/>
				</VBox>
				<ToolbarSpacer/>
				<HBox class="sapUiLargeMarginBegin" >
				<CheckBox text="Linked To Payment"/>
				</HBox>
			</HBox>
		</CustomListItem>
	</List>

  <beginButton>
            <Button text="{i18n>lblOk}" press="onCreateParamOk" />
        </beginButton>
        <endButton>
            <Button text="{i18n>lblCancel}" press="onCreateParamCancel" />
        </endButton>
 </Dialog>





</c:FragmentDefinition>
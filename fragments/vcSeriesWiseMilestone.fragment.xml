<c:FragmentDefinition xmlns="sap.m" xmlns:u="sap.ui.unified" xmlns:l="sap.ui.layout" xmlns:c="sap.ui.core">
		<Table
		items="{vendorContractModel>/mileMpml2List}" >
		<infoToolbar>
			<Toolbar>
				<Label text="{parts:[{value:'lblEpiMpmData'},{path:'artistContractModel>/Cnttp'}],formatter:'.Formatter.formatLabelContentBased'}"/>
				<ToolbarSpacer></ToolbarSpacer>
			</Toolbar>
		</infoToolbar>
		<columns>
			<Column mergeDuplicates="true">
				<header>
					<Text text="Series" />
				</header>
			</Column>
			<Column mergeDuplicates="false">
				<header>
					<Text text="Milestone" />
				</header>
			</Column>
			<Column
				minScreenWidth="Tablet"
				demandPopin="true"
				 >
				<header>
					<Text text="Version" />
				</header>
			</Column>
			<Column
				minScreenWidth="Tablet"
				demandPopin="true"
				 >
				<header>
					<Text text="{parts:[{value:'lblTblColEpisodesYearWise'},{path:'vendorContractModel>/Cnttp'}],formatter:'.Formatter.formatLabelContentBased'}" />
				</header>
			</Column>
			<Column >
				<header>
					<Text text="Amount Due" />
				</header>
			</Column>
		</columns>
		<ColumnListItem >
			<Text text="{vendorContractModel>Mpml2}" />
			<Text text="{vendorContractModel>Msidnm}" />
			<Text text="{vendorContractModel>Dmver}" />
			<Text text="{vendorContractModel>Epiid}" class="sapUiSizeCompact"/>
			<Label text="{path:'vendorContractModel>Dueamt', type:'sap.ui.model.type.Float',formatOptions: { maxFractionDigits: 2 }}"></Label>
		</ColumnListItem>
	</Table>
</c:FragmentDefinition>